<% @page_title="Edit Admin User: #{@admin_user.name}" %>
<%= link_to("<< Назад к списку", {:action=> 'list'}, :class => 'back-link') %>

<div class="user edit">
  <h2>Edit Admin User</h2>

  <%= form_for(:admin_user, :url=>{:action=> 'update', :id=> @admin_user.id
  }) do |f| %>

      <%= render(:partial => "form", :locals => {:f=> f}) %>

      <div class="button-container">

        <%= submit_tag('Обновить') %>
      </div>
  <% end %>
</div>