    width: 40px;
    margin-right: -40px;
    height: 40px;
    background: no-repeat url("<%= asset_path 'cornerRT.png' %>");

}

#ornament-top {
    width: 100%;
    height: 40px;
    background: url("<%= asset_path 'top.png' %>");

}

#ornament-corner-BL {
    margin-left: -40px;
